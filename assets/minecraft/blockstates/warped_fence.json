{
  "multipart": [
    {
      "when": {
        "north": "false", "south": "false", "west": "false", "east": "false"
      },	
      "apply": {
        "model": "tlaminecraft:industrial/truss_beam"
      }
    },
    {
      "when": {
        "north": "true"
      },
      "apply": {
        "model": "tlaminecraft:industrial/truss_beam_side",
        "uvlock": true
      }
    },
    {
      "when": {
        "east": "true"
      },
      "apply": {
        "model": "tlaminecraft:industrial/truss_beam_side",
        "y": 90,
        "uvlock": true
      }
    },
    {
      "when": {
        "south": "true"
      },
      "apply": {
        "model": "tlaminecraft:industrial/truss_beam_side",
        "y": 180,
        "uvlock": true
      }
    },
    {
      "when": {
        "west": "true"
      },
      "apply": {
        "model": "tlaminecraft:industrial/truss_beam_side",
        "y": 270,
        "uvlock": true
      }
    }
  ]
}